    "import pyspark\n",
    "if not 'sc' in globals():\n",
    "    sc = pyspark.SparkContext()\n",
    "\n",
    "lines = sc.textFile(\"Spark File Words.ipynb\")\n",
    "lineLengths = lines.map(lambda s: len(s))\n",
    "totalLength = lineLengths.reduce(lambda a, b: a + b) \n",
    "print(totalLength)"
   ]
